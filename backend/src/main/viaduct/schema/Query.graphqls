extend type Query @scope(to: ["default"]) {
  """
  Get all checklist items for the authenticated user.
  """
  checklistItems: [ChecklistItem!]! @resolver
}

extend type Query @scope(to: ["admin"]) {
  """
  Get all users in the system. Only admins can view all users.
  """
  users: [User!]! @resolver
}
